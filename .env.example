# 服务配置
PORT=5678
ALLOWED_DOMAIN=*
PROXY_URL=

# 功能配置
ENABLE_FLAC=true

# 安全配置
JWT_SECRET=your-jwt-secret-key-here
API_KEY=

# 性能配置
MAX_CONNECTIONS=1000
TIMEOUT_SECONDS=30
CACHE_TTL=300
WORKER_POOL_SIZE=10
RATE_LIMIT=100

# 监控配置
LOG_LEVEL=info
METRICS_ENABLED=true
TRACING_ENABLED=false
HEALTH_CHECK_INTERVAL=30

# 第三方API配置
# UNM服务器配置
UNM_SERVER_BASE_URL=https://your-unm-server.com
UNM_SERVER_API_KEY=

# GDStudio API配置
GDSTUDIO_BASE_URL=https://your-gdstudio-api.com
GDSTUDIO_API_KEY=

# 数据库和Redis配置已移除 - 项目使用内存缓存，无需数据库

# 环境标识
GO_ENV=development
